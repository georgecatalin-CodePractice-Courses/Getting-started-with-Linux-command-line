    .path-label {margin-bottom: 5px; font-weight: 500; font-size: 12px;}
    .path-courses {font-size: 14px; font-weight: 500; line-height: 20px; color: #aaaaaa;}
    .path-courses span {color: white; font-weight: 500;}
    .path-link {margin-top: auto; font-size: 16px; font-weight: 700; line-height: 36px; color: white;}
    .simple-arrow:after {
        content: "\f105";
        margin-left: 4px;
        font-family: FontAwesome;
        color: #EC008C;
        transition: 0.3s all;
    }
    .path-items-container a:hover .simple-arrow:after {margin-left: 8px;}

    @media only screen and (max-width: 575px) {
        .paths-items {
            flex-basis:100%;
            padding-top: 10px;
            padding-left: 0;
            padding-right: 0;
       }
    }
    @media only screen and (min-width: 1025px) {
        .paths-block {margin-left: -20px; margin-right: -20px; }
    }

@media all and (-ms-high-contrast: none),
(-ms-high-contrast: active) {
       .paths-block {display: -ms-flexbox;}
       .paths-items {-ms-flex: 1 0 40%;}
}
@media all and (max-width: 575px) and (-ms-high-contrast: none),
(-ms-high-contrast: active) {
       .paths-items {-ms-flex: 1 0 100%;}
}
</style>

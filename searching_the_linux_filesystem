jQuery(".text.section, .text-2.section, .text-3.section, .text-4.section, .title.section, .title2.section").each(function(key,item) {
    if (item.innerHTML.indexOf("\u2028") > -1){
        item.innerHTML = item.innerHTML.replace(/\u2028/g, ' ');
    }
});
